{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Rinstall",
  "version": "0.2.0",
  "identifier": "io.github.yuerei.rinstall",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000/login",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run generate"
  },
  "app": {
    "windows": [
      {
        "title": "Rinstall",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "devtools": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png",
      "icons/icon.ico",
      "icons/icon.icns"
    ]
  }
}
